
public class Nodo {
   
    
    //Vamos hacer una lista enlazada que contiene solo numereso enteros
    public int dato;
    public Nodo siguiente; //PUntero enlace,para poder ir uniendo los difeentes nodos.
    
    Object dato2;
    
    //constructor para insertar al final
    public Nodo(int d){ //para cuando vamos a crear un NOdo al final. 
        this.dato =d;
        this.siguiente=null;
    }
    
    public Nodo(Object dato2){
        this.dato=dato;
    }
   
    
}

